    <!-- Map 1 -->
    <script>function initialize() {
		var myOptions = {
			zoom: 16,
			center: new google.maps.LatLng(51.517319, -0.142791),
			mapTypeId: google.maps.MapTypeId.ROADMAP,
			styles: [
				{"featureType": "landscape", "stylers": [
				  { "visibility": "on" },
				  { "color": "#4b4b50" },
				  ]
				},
				{"featureType": "road", "stylers": [
                    { "color": "#b5d334" }
                  ]
                },
				{"featureType":"water","stylers":[
					{"hue":"#00B6FF"},
					{"saturation":4.2},
					{"lightness":-63.4},
					{"gamma":1}
				]},
				{"featureType":"poi","stylers":[
					{"hue":"#9FFF00"},
					{"saturation":0},
					{"lightness":0},
					{"gamma":1}]
					}
			]
		};

		var map4 = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
		var image = "http://carawebs.com/maptest/img/expedition-map-pin.png";
		var myLatLng = new google.maps.LatLng(51.517319, -0.14279);
		var beachMarker = new google.maps.Marker({
		position: myLatLng,
		map: map4,
		icon: image
		});
		}
		google.maps.event.addDomListener(window, "load", initialize);
		</script>
		<!-- Map 2 -->		
		<script>
		function initialize() {
		var myOptions = {
			zoom: 16,
			center: new google.maps.LatLng(51.517319, -0.142791),
			mapTypeId: google.maps.MapTypeId.ROADMAP,
			styles: [{"featureType":"landscape","stylers":[{"saturation":-100},{"lightness":65},{"visibility":"on"}]},{"featureType":"poi","stylers":[{"saturation":-100},{"lightness":51},{"visibility":"simplified"}]},{"featureType":"road.highway","stylers":[{"saturation":-100},{"visibility":"simplified"}]},{"featureType":"road.arterial","stylers":[{"saturation":-100},{"lightness":30},{"visibility":"on"}]},{"featureType":"road.local","stylers":[{"saturation":-100},{"lightness":40},{"visibility":"on"}]},{"featureType":"transit","stylers":[{"saturation":-100},{"visibility":"simplified"}]},{"featureType":"administrative.province","stylers":[{"visibility":"off"}]},{"featureType":"water","elementType":"labels","stylers":[{"visibility":"on"},{"lightness":-25},{"saturation":-100}]},{"featureType":"water","elementType":"geometry","stylers":[{"hue":"#ffff00"},{"lightness":-25},{"saturation":-97}]}]
		};

		var map2 = new google.maps.Map(document.getElementById("map_canvas_2"), myOptions);
		//
		var image = "http://davidegan.me/wp-content/uploads/2014/03/expedition_website_logo.png";
		var myLatLng = new google.maps.LatLng(51.517319, -0.14279);
		var beachMarker = new google.maps.Marker({
		position: myLatLng,
		map: map2,
		icon: image
		});
		}
		google.maps.event.addDomListener(window, "load", initialize);
		</script>
		<!-- Map 3 -->		
		<script>
		function initialize() {
		var myOptions = {
			zoom: 16,
			center: new google.maps.LatLng(51.517319, -0.142791),
			mapTypeId: google.maps.MapTypeId.ROADMAP,
			styles: [{"featureType":"all","stylers":[{"saturation":-100},{"gamma":0.5}]}]
		};

		var map3 = new google.maps.Map(document.getElementById("map_canvas_3"), myOptions);
		//
		var image = "http://davidegan.me/wp-content/uploads/2014/03/expedition_website_logo.png";
		var myLatLng = new google.maps.LatLng(51.517319, -0.14279);
		var beachMarker = new google.maps.Marker({
		position: myLatLng,
		map: map3,
		icon: image
		});
		}
		google.maps.event.addDomListener(window, "load", initialize);
		</script>
		
		<!-- Map 4 -->
		<script>
		function initialize() {
		var mapOptions = {
			zoom: 16,
			center: new google.maps.LatLng(51.517319, -0.142791)
		}
		var map = new google.maps.Map(document.getElementById('map_canvas_4'), mapOptions);
		var image = 'http://davidegan.me/wp-content/uploads/2014/03/expedition_website_logo.png';
		var myLatLng = new google.maps.LatLng(51.517319, -0.14279);
		var beachMarker = new google.maps.Marker({
		position: myLatLng,
		map: map,
		icon: image
		});
		}
		google.maps.event.addDomListener(window, 'load', initialize);
		</script>
    

