		<!-- Map 1 -->
    <script>
		var map5;
		function initialize() {
		var myOptions = {
			zoom: 12,
			center: new google.maps.LatLng(52.84021,-8.975243),
			mapTypeId: google.maps.MapTypeId.ROADMAP,
			scrollwheel: false,
			disableDefaultUI: true,
			styles: [
				{"featureType": "road", "elementType": "geometry.fill", "stylers": [
					  { "color": "#4b4b50" },
					  { "visibility": "on" },
					  { "weight": 5 }]},
				{"featureType": "landscape", "stylers": [
				  { "visibility": "on" },
				  { "color": "#a5a5a5" },
				  ]
				},
				{"featureType": "transit.line", "stylers": [
					  { "visibility": "off" }
					]
				},
				{"featureType": "road", "elementType": "labels.text", "stylers": [
				  { "visibility": "on" },
				  { "invert_lightness": true },
				  { "weight": 0.4 },
				  { "color": "#ffffff" }
				]
				},
				{"featureType":"water","stylers":[
					{"hue":"#00B6FF"},
					{"saturation":4.2},
					{"lightness":-63.4},
					{"gamma":1}
				]},
				{"featureType":"poi","stylers":[
					{ "visibility": "off" }]
					}
			]
		};

		map5 = new google.maps.Map(document.getElementById("map_canvas_5"), myOptions);
		var image = "http://carawebs.com/maptest/img/expedition-map-pin.png";
		var myLatLng = new google.maps.LatLng(51.5172,-0.1426);
		var beachMarker = new google.maps.Marker({
		position: myLatLng,
		map: map5,
		icon: image
		});
		}
		google.maps.event.addDomListener(window, "load", initialize);
		google.maps.event.addDomListener(window, "resize", function() {
			 var center = map5.getCenter();
			 google.maps.event.trigger(map5, "resize");
			 map5.setCenter(center); 
			});
		</script>
